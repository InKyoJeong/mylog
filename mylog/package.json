{
  "name": "mylog",
  "version": "1.5.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "react-native setup-ios-permissions && pod-install",
    "appcenter-key:android": "appcenter codepush deployment list -a koy/mylog-android -k",
    "appcenter-key:ios": "appcenter codepush deployment list --app koy/mylog-ios -k",
    "codepush:android": "appcenter codepush release-react -a koy/mylog-android -d Staging -s -o ./build -m -t 1.5",
    "codepush:ios": "appcenter codepush release-react -a koy/mylog-ios -d Staging -s -o ./build -m -t 1.5",
    "codepush-history:android": "appcenter codepush deployment history -a koy/mylog-android Production",
    "codepush-history:ios": "appcenter codepush deployment history -a koy/mylog-ios Production",
    "bundle-android": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/index.android.bundle --dev false --entry-file index.js --platform android --sourcemap-output build/CodePush/index.android.bundle.map",
    "bundle-ios": "react-native bundle --assets-dest build/CodePush --bundle-output build/CodePush/main.jsbundle --dev false --entry-file index.js --platform ios --sourcemap-output build/CodePush/main.jsbundle.map"
  },
  "dependencies": {
    "@invertase/react-native-apple-authentication": "^2.2.2",
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-community/geolocation": "^3.0.6",
    "@react-native-community/slider": "^4.4.2",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "@sentry/react-native": "^5.6.0",
    "@tanstack/react-query": "^4.28.0",
    "appcenter": "^4.4.5",
    "appcenter-analytics": "^4.4.5",
    "appcenter-crashes": "^4.4.5",
    "axios": "^1.3.4",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.7",
    "react-native": "0.71.4",
    "react-native-code-push": "^8.0.2",
    "react-native-config": "^1.5.0",
    "react-native-date-picker": "^4.2.13",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-crop-picker": "^0.39.0",
    "react-native-map-clustering": "git+https://github.com/InKyoJeong/react-native-map-clustering.git",
    "react-native-maps": "^1.5.0",
    "react-native-permissions": "^3.8.0",
    "react-native-reanimated": "^3.1.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^12.1.0",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.8",
    "pod-install": "0.1.38",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "reactNativePermissionsIOS": [
    "LocationAccuracy",
    "LocationAlways",
    "LocationWhenInUse",
    "MediaLibrary",
    "Notifications",
    "PhotoLibrary",
    "PhotoLibraryAddOnly"
  ],
  "jest": {
    "preset": "react-native",
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/utils.ts"
    ]
  },
  "packageManager": "yarn@1.22.19"
}
